<h1>Buildings Index</h1>

<% buildings.each do |building| %>
  <div>
    <p>Street Address: <%= building.street_address %></p>
    <p>City: <%= building.city %></p>
    <p>State: <%= building.state_name %></p>
    <p>Postal Code: <%= building.postal_code %></p>
    <p>Description: <%= building.description %></p>
    <p>Owner: <%= building.owner_name %></p>
  </div>
<% end %>

<%= link_to 'Create Building', new_building_path %>
